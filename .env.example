# AI Provider Configuration
# Choose which AI provider to use: "gemini" or "openai"
AI_PROVIDER=gemini

# Gemini API Configuration (required if AI_PROVIDER=gemini)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API Configuration (required if AI_PROVIDER=openai)
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Application URLs
NEXT_PUBLIC_APP_URL=https://examgenie.app

# RevenueCat Webhook Configuration (for subscription management)
REVENUECAT_PUBLIC_KEY=your_revenuecat_public_key_here

# Prompt logging (localhost only)
ENABLE_PROMPT_LOGGING=true
